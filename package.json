{
  "name": "xfinance",
  "version": "1.0.0",
  "license": "MIT",
  "main": "electron.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane-React-JS.git"
  },
  "config": {
    "app_to_debug": "excel",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "cross-env ANALYZE=false webpack --mode production",
    "build:dev": "cross-env ANALYZE=false webpack --mode development",
    "dev-server": "cross-env ANALYZE=false webpack serve --mode development --port 3000",
    "analyze": "cross-env ANALYZE=true webpack --mode production",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "signin": "office-addin-dev-settings m365-account login",
    "signout": "office-addin-dev-settings m365-account logout",
    "start": "cross-env ANALYZE=false office-addin-debugging start manifest.xml",
    "dev": "concurrently \"npm run start\" \"npm --prefix backend start\"",
    "start:desktop": "cross-env ANALYZE=false office-addin-debugging start manifest.xml desktop",
    "start:web": "cross-env ANALYZE=false office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "watch": "cross-env ANALYZE=false webpack --mode development --watch",
    "build:web": "webpack --mode production",
    "build:desktop": "npm run build:web && electron-builder"
  },
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "@fluentui/react-calendar-compat": "^0.2.7",
    "@fluentui/react-components": "^9.47.2",
    "@fluentui/react-icons": "^2.0.233",
    "@microsoft/office-js": "^1.1.108",
    "buffer": "^6.0.3",
    "core-js": "^3.36.0",
    "cors": "^2.8.5",
    "debug": "^4.4.1",
    "dotenv": "^16.5.0",
    "es6-promise": "^4.2.8",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "regenerator-runtime": "^0.14.1",
    "serverless-http": "^4.0.0",
    "xlsx": "^0.18.5",
     "cross-env": "^7.0.3",
  "webpack": "^5.95.0",
  "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@types/es6-collections": "^0.5.36",
    "@types/office-js": "^1.0.377",
    "@types/office-runtime": "^1.0.35",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/webpack": "^5.28.5",
    "acorn": "^8.11.3",
    "babel-loader": "^9.1.3",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "eslint-plugin-office-addins": "^3.0.2",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^6.2.0",
    "find-process": "^1.4.4",
    "gh-pages": "^6.3.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "office-addin-cli": "^1.6.5",
    "office-addin-debugging": "^2.1.12",
    "office-addin-dev-certs": "^1.13.5",
    "office-addin-lint": "^2.3.5",
    "office-addin-manifest": "^1.13.6",
    "office-addin-prettier-config": "^1.2.1",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "source-map-loader": "^5.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.2",
    
    "webpack-bundle-analyzer": "^4.10.2",

    "webpack-dev-server": "^5.2.2"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "last 2 versions",
    "not ie <= 11",
    "not dead"
  ],
  "build": {
    "appId": "com.xfinance.desktop",
    "productName": "XFinance",
    "files": [
      "dist/**/*",
      "electron.js"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "artifactName": "XFinance-Setup-${version}.exe"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false,
      "deleteAppDataOnUninstall": false
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
