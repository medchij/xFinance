{
 "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "vercel/node@20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/XFinance.html",
      "permanent": true
    }
  ]
}
