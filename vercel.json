{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      },
      "env": {
        "REACT_APP_API_URL": "@react_app_api_url"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/XFinance.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
