{
  "version": 2,
  "functions": {
    "backend/server.js": {
      "runtime": "nodejs18.x",
      "includeFiles": [
        "backend/dataNany/**",
        "backend/dataMall/**",
        "backend/dataSoyombo/**",
        "backend/config/current-env.json"
      ]
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/server.js"
    }
  ]
}
