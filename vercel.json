{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "nodejs18.x",
      "includeFiles": [
        "backend/dataNany/*.json",
        "backend/dataMall/*.json",
        "backend/config/current-env.json"
      ]
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server.js"
    }
  ]
}
