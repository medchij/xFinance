{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "nodejs18.x",
      "includeFiles": [
        "backend/config/current-env.json",
        "backend/dataMall/*.json",
        "backend/dataNany/*.json",
        "backend/dataSoyombo/*.json"
        "backend/data/*.json"
      ]
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/server.js" }
  ]
}
